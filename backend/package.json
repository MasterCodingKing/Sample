{
  "name": "barangay-management-backend",
  "version": "1.0.0",
  "description": "Backend API for Barangay Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node scripts/setup-database.js",
